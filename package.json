{
  "name": "joplin-plugin-sortable-tables",
  "version": "1.0.0",
  "scripts": {
    "dist": "webpack --env joplin-plugin-config=buildMain && webpack --env joplin-plugin-config=buildExtraScripts && webpack --env joplin-plugin-config=createArchive",
    "prepare": "pnpm run dist",
    "update": "pnpm install -g generator-joplin && yo joplin --node-package-manager pnpm --update --force",
    "updateVersion": "webpack --env joplin-plugin-config=updateVersion"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin",
    "markdown",
    "table",
    "sorting",
    "sortable"
  ],
  "homepage": "https://github.com/parkersprouse/joplin-plugin-sortable-tables",
  "repository": {
    "type": "git",
    "url": "https://github.com/parkersprouse/joplin-plugin-sortable-tables"
  },
  "devDependencies": {
    "@joplin/lib": "~2.14.1",
    "@types/node": "^20.12.7",
    "chalk": "^5.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "on-build-webpack": "^0.1.0",
    "tar": "^7.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  },
  "files": [
    "publish"
  ]
}
